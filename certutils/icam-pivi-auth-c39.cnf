[ ca ]
default_ca = ca_default

[ ca_default ]
dir = .
database = ${dir}/database/pivi-gen3-index.txt 
unique_subject = no
new_certs_dir = .
private_key = ${dir}/pem/ICAM_Test_Card_PIV-I_Signing_CA_-_gold_gen3.private.pem 
certificate = ${dir}/pem/ICAM_Test_Card_PIV-I_Signing_CA_-_gold_gen3.crt 
name_opt = ca_default
x509_extensions = ee_ext
default_days = 2190 
default_md = sha256
serial = ${dir}/database/pivi-gen3-serial
policy = policy_match
email_in_dn = no
crl_extensions = crl_ext

[ policy_match ]
C = match
O = optional
OU = optional
CN = optional
emailAddress = optional

[ crl_ext ]
authorityKeyIdentifier=keyid:always,issuer:always

[ req ]
prompt = yes
distinguished_name = req_distinguished_name
attributes = req_attributes
req_extensions = v3_req

[ req_distinguished_name ]
C = Country Name
C_default = US
O = Organization
O_default = U.S. Government 
OU = Organizational Unit
OU_default = ICAM Test Cards
CN = Common Name (if Authentication)
CN_default = ICAM Test Card PIV-I Auth Fed SP 800-73-4

[ req_attributes ]

[ v3_req ]
basicConstraints = CA:FALSE
subjectKeyIdentifier = hash

[ ee_ext ]
authorityKeyIdentifier = keyid:issuer
subjectKeyIdentifier = hash
keyUsage = critical, digitalSignature
extendedKeyUsage = 1.3.6.1.4.1.311.20.2.2, 1.3.6.1.5.5.7.3.2, 1.3.6.1.5.2.3.4, 2.5.29.37.0
authorityInfoAccess = caIssuers;URI:http://http.apl-test.cite.fpki-lab.gov/aia/certsIssuedToPIV-ISigningCA.p7c, OCSP;URI:http://ocsp-pivi.apl-test.cite.fpki-lab.gov
crlDistributionPoints = URI:http://http.apl-test.cite.fpki-lab.gov/crls/ICAMTestCardPIV-ISigningCA.crl

certificatePolicies = ia5org,2.16.840.1.101.3.2.1.48.248
subjectAltName = @subjectAltName_sect

[ subjectAltName_sect ]
#otherName = 2.16.840.1.101.3.6.6;FORMAT:HEX,OCT:D1:38:10:D8:28:AF:2C:10:84:39:9D:A1:68:58:28:AF:02:10:87:33:84:E7:39:C3:EB
URI = urn:uuid:af8b1780-863c-42af-8925-02206f311a28
