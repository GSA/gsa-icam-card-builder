[ ca ]
default_ca = ca_default

[ ca_default ]
dir = .
database = database/index.txt 
unique_subject = no
new_certs_dir = .
private_key = pem/ICAM_Test_Card_PIV_Signing_CA_-_gold_gen3.private.pem 
certificate = pem/ICAM_Test_Card_PIV_Signing_CA_-_gold_gen3.crt 
name_opt = ca_default
x509_extensions = ee_ext
default_days = 2190 
default_md = sha256
serial = database/serial
policy = policy_match
email_in_dn = no

[ policy_match ]
C = match
O = optional
OU = optional
CN = optional
emailAddress = optional
serialNumber = optional

[ req ]
prompt = yes
distinguished_name = req_distinguished_name
attributes = req_attributes
req_extensions = v3_req

[ req_distinguished_name ]
C = Country Name
C_default = US
O = Organization
O_default = U.S. Government 
OU = Organizational Unit
OU_default = ICAM Test Cards
serialNumber = 200-bit FASC-N (if Card Auth)
serialNumber_default = D13810D828AF2C108424ADA1685828AF0210849584E739C3EB

[ req_attributes ]

[ v3_req ]
basicConstraints = CA:FALSE
subjectKeyIdentifier = hash

[ ee_ext ]
authorityKeyIdentifier = keyid:issuer
subjectKeyIdentifier = hash
keyUsage = critical, digitalSignature
extendedKeyUsage = critical,2.16.840.1.101.3.6.8
authorityInfoAccess = caIssuers;URI:http://http.apl-test.cite.fpki-lab.gov/aia/certsIssuedToICAMTestCardSigningCA.p7c, OCSP;URI:http://ocspInvalidSigResponder.apl-test.cite.fpki-lab.gov
crlDistributionPoints = URI:http://http.apl-test.cite.fpki-lab.gov/crls/ICAMTestCardSigningCA.crl
2.16.840.1.101.3.6.9.1 = DER:01:01:00
certificatePolicies = ia5org,2.16.840.1.101.3.2.1.48.13
subjectAltName = @subjectAltName_sect

[ subjectAltName_sect ]
otherName = 2.16.840.1.101.3.6.6;FORMAT:HEX,OCT:D1:38:10:D8:28:AF:2C:10:84:24:AD:A1:68:58:28:AF:02:10:84:95:84:E7:39:C3:EB
URI = urn:uuid:e952102e-3be4-4ca8-99f7-46086678c019
